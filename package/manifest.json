{
    "manifest_version": 3,
    "name": "国开小助手",
    "version": "1.1.2",
    "permissions": ["contextMenus", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "web_accessible_resources": [
        {
            "resources": ["main.js", "js/*"],
            "matches": ["https://lms.ouchn.cn/*"]
        }
    ]
}